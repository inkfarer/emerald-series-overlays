{
    "name": "emerald-series-overlays",
    "version": "0.2.3",
    "author": {
        "name": "inkfarer",
        "email": "inkfarer@inkfarer.com",
        "url": "https://inkfarer.com"
    },
    "contributors": [
        "BrushStrokes (https://brush.rip/)"
    ],
    "nodecg": {
        "compatibleRange": "^1.7.0",
        "mount": [
            {
                "directory": "assets",
                "endpoint": "assets"
            }
        ],
        "dashboardPanels": [
            {
                "name": "teams",
                "title": "Teams",
                "width": 3,
                "file": "teams.html",
                "workspace": "VCA Setup",
                "headerColor": "#262F40"
            },
            {
                "name": "tournament",
                "title": "Tournament Info",
                "width": 2,
                "file": "tournament.html",
                "workspace": "VCA Setup",
                "headerColor": "#262F40"
            },
            {
                "name": "intermission",
                "title": "Intermission",
                "width": 2,
                "file": "intermission.html",
                "workspace": "VCA",
                "headerColor": "#262F40"
            },
            {
                "name": "activematch",
                "title": "Active Match",
                "width": 2,
                "file": "activematch.html",
                "workspace": "VCA",
                "headerColor": "#262F40"
            },
            {
                "name": "nextmatch",
                "title": "Next Match",
                "width": 2,
                "file": "nextmatch.html",
                "workspace": "VCA",
                "headerColor": "#262F40"
            },
            {
                "name": "casters",
                "title": "Casters",
                "width": 2,
                "file": "casters.html",
                "workspace": "VCA",
                "headerColor": "#262F40"
            }
        ],
        "graphics": [
            {
                "file": "intermission.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "rosters.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "maps.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "gameplay.html",
                "width": 1920,
                "height": 1080
            },
            {
                "file": "commentary.html",
                "width": 1920,
                "height": 1080
            }
        ]
    },
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "build:dev": "webpack",
        "build:watch": "webpack -w",
        "build:clean": "npm run clean && npm run build",
        "clean": "trash node_modules/.cache && trash dashboard && trash extension && trash graphics",
        "start": "node ../..",
        "schema-types": "nodecg schema-types",
        "lint": "eslint --ext .js,.vue,.ts ./src",
        "lint:fix": "eslint ./src --fix"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.1.1",
        "@fortawesome/free-brands-svg-icons": "^6.1.1",
        "@fortawesome/free-solid-svg-icons": "^6.1.1",
        "@fortawesome/vue-fontawesome": "^3.0.0-5",
        "@iplsplatoon/vue-components": "^2.8.0",
        "@types/lodash": "^4.14.182",
        "@types/webpack": "^5.28.0",
        "@types/webpack-livereload-plugin": "^2.3.3",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.16.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-vue": "^9.0.1",
        "fork-ts-checker-webpack-plugin": "^7.2.11",
        "globby": "^11.0.3",
        "gsap": "npm:@gsap/shockingly@^3.10.2",
        "html-webpack-plugin": "^5.5.0",
        "lodash": "^4.17.21",
        "pinia": "^2.0.14",
        "sass": "^1.52.1",
        "sass-loader": "^13.0.0",
        "style-loader": "^3.3.1",
        "trash-cli": "^5.0.0",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.1",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "^4.7.2",
        "vue": "3.2.26",
        "vue-loader": "^17.0.0",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "webpack-livereload-plugin": "^3.0.2",
        "webpack-node-externals": "^3.0.0"
    }
}
