{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "required": ["flavorText", "showNextMatch"],
    "properties": {
        "flavorText": {
            "type": "string"
        },
        "showNextMatch": {
            "type": "boolean"
        }
    },
    "default": {
        "flavorText": "Starting soon...",
        "showNextMatch": false
    }
}
