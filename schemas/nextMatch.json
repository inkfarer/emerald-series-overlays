{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "required": ["teamA", "teamB", "games", "match"],
    "properties": {
        "teamA": {
            "$ref": "defs/team.json#/team"
        },
        "teamB": {
            "$ref": "defs/team.json#/team"
        },
        "match": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                }
            },
            "required": ["name"]
        },
        "games": {
            "type": "array",
            "additionalItems": false,
            "items": {
                "type": "object",
                "properties": {
                    "map": {
                        "type": "string"
                    }
                },
                "required": [
                    "map"
                ]
            }
        }
    },
    "default": {
        "teamA": {
            "id": "aaa111",
            "name": "Example team 1",
            "players": [
                { "id": "aaa1111", "name": "Player One" },
                { "id": "aaa1112", "name": "Player Two" }
            ]
        },
        "teamB": {
            "id": "bbb222",
            "name": "Example team 2",
            "players": [
                { "id": "bbb2221", "name": "Player Three" },
                { "id": "bbb2222", "name": "Player Four" }
            ]
        },
        "match": {
            "name": "Round One"
        },
        "games": [
            {
                "map": "Unknown"
            },
            {
                "map": "Unknown"
            },
            {
                "map": "Unknown"
            }
        ]
    }
}
